  AssertEqual 'csInterpolationDelimiter', SyntaxAt(9)
  AssertEqual 'csInterpolationDelimiter', SyntaxAt(14)
  AssertEqual 'csNumber',                 SyntaxAt(15)
  AssertEqual 'csInterpolationDelimiter', SyntaxAt(16)

